{
  "expo": {
    "name": "PlanetRate",
    "slug": "PlanetRate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/pr_icon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.jpeg",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.v-hiculemedia.magnuspoirier",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la caméra pour numériser des code-bars de forme QR."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "android": {
      "package": "com.vhiculemedia.magnuspoirier"
    }
  }
}